<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Main\PhotoBundle\Entity\Album" table="album">
    <indexes>
      <index name="idx_39986e439395c3f3" columns="customer_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="album_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="albumTitle" type="string" column="albumtitle" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCreated" type="datetime" column="datecreated" nullable="false"/>
    <many-to-one field="customer" target-entity="Main\BackendBundle\Entity\Customer" fetch="LAZY">
      <join-columns>
        <join-column name="customer_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
