<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Main\CityBundle\Entity\AllCities" table="all_cities">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="all_cities_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="city" type="string" column="city" length="33" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cityAscii" type="string" column="cityascii" length="39" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iso2" type="string" column="iso2" length="3" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iso3" type="string" column="iso3" length="3" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="province" type="string" column="province" length="43" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
