<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Main\BackendBundle\Entity\CustomerCity" table="customer_city">
    <indexes>
      <index name="idx_79caa6439395c3f3" columns="customer_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="customer_city_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="cityName" type="string" column="city_name" length="59" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="logitude" type="float" column="logitude" precision="10" scale="0" nullable="false"/>
    <field name="latitude" type="float" column="latitude" precision="10" scale="0" nullable="false"/>
    <field name="cityBrief" type="text" column="city_brief" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateArrived" type="datetime" column="date_arrived" nullable="false"/>
    <field name="dateDeparted" type="datetime" column="date_departed" nullable="false"/>
    <field name="currentCity" type="boolean" column="current_city" nullable="false"/>
    <field name="visitedCity" type="boolean" column="visited_city" nullable="false"/>
    <field name="livedCity" type="boolean" column="lived_city" nullable="false"/>
    <many-to-one field="customer" target-entity="Customer" fetch="LAZY">
      <join-columns>
        <join-column name="customer_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
